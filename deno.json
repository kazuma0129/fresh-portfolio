{
  "tasks": {
    "start": "deno run -A --watch=static/,routes/ dev.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts",
    "test": "deno test tests/ -A"
  },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.7.3/",
    "assert": "jsr:@std/assert@^1.0.0",
    "frontmatter": "https://deno.land/x/frontmatter@v0.1.4/mod.ts",
    "@std/front-matter": "jsr:@std/front-matter@^1.0.5",
    "marked": "https://esm.sh/marked@12.0.0",
    "dompurify": "https://esm.sh/dompurify@3.0.8",
    "preact": "https://esm.sh/preact@10.24.3",
    "preact-render-to-string": "https://esm.sh/preact-render-to-string@6.5.11?deps=preact@10.24.3",
    "preact/": "https://esm.sh/preact@10.24.3/"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "lint": {
    "exclude": ["_fresh/"]
  }
}
